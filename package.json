{
  "name": "strikethrough-toggle",
  "displayName": "Strikethrough Toggle",
  "description": "Toggle strikethrough on the current line in Markdown files",
  "version": "0.0.1",
  "engines": {
      "vscode": "^1.60.0"
  },
  "categories": [
      "Other"
  ],
  "activationEvents": [
      "onCommand:extension.toggleStrikethrough"
  ],
  "main": "./out/extension.js",
  "contributes": {
      "commands": [{
          "command": "extension.toggleStrikethrough",
          "title": "Toggle Strikethrough"
      }]
  },
  "scripts": {
      "vscode:prepublish": "npm run compile",
      "compile": "tsc -p ./",
      "watch": "tsc -watch -p ./",
      "pretest": "npm run compile && npm run lint",
      "lint": "eslint src --ext ts"
  },
  "devDependencies": {
      "@types/vscode": "^1.60.0",
      "@types/node": "^14.x.x",
      "@typescript-eslint/eslint-plugin": "^4.16.0",
      "@typescript-eslint/parser": "^4.16.0",
      "eslint": "^7.21.0",
      "typescript": "^4.3.5"
  }
}
